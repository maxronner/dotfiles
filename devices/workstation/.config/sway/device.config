output DP-1 {
	mode 2560x1440@143.998Hz
	pos 1080 2560
	adaptive_sync fullscreen
}

output HDMI-A-1 {
	mode 1920x1080@75.001Hz
	transform 270
	pos 0 2200
}
workspace 10 output HDMI-A-1

exec_always swaybg --output DP-1 --color 000000 --image ~/.config/sway/1.wallpaper --mode fill \
--output HDMI-A-1 --color 000000

exec ./fullscreen_cursor_watch.sh
